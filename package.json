{
  "name": "rollup-todomvc",
  "author": "Cypress DX Team",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "start": "http-server",
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:record": "cypress run --record --key",
    "combine:report": "mochawesome-merge ./mochawesome-report/*.json -o index.json",
    "generate:report": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "dependencies": {
    "todomvc-app-css": "2.3.0",
    "todomvc-common": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-babel": "5.3.0",
    "cypress": "^7.0.0",
    "http-server": "^0.12.3",
    "rollup": "2.44.0",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "mochawesome-report-generator": "5.1.0"
  }
}
